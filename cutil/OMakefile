#
# LibMojave C files
#
CFILES[] =
    lm_abstract
    lm_float
    lm_ncurses
    lm_printf
    lm_raw
    lm_readline
    lm_rusage
    lm_terminfo
    lm_mmap
    lm_marshal_shared
    lm_ssl
    lm_config
    lm_uname_ext
    lm_channel
    lm_ctype
    lm_nocompare

#
# Libraries
#
if $(defined LMINSTALL)
    StaticCLibraryInstall($(LMINSTALL), $(LIB), liblmcutil, $(CFILES))
else
    StaticCLibrary(liblmcutil, $(CFILES))

if $(LM_STANDALONE)
    .DEFAULT: liblmcutil$(EXT_LIB)

clean:
    $(CLEAN)
